{
    "name": "valheureux/radisse",
    "description": "Application created to manage the local currency of the province of Liège, Belgium",
    "license": "AGPL-3.0",
    "type": "project",
    "support": {
        "source": "https://github.com/monnaie-valheureux/radisse",
        "issues": "https://github.com/monnaie-valheureux/radisse/issues"
    },
    "authors": [
        {
            "name": "Michaël Lecerf",
            "email": "michael@estsurinter.net"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "commerceguys/addressing": "^0.8.4",
        "commerceguys/intl": "^0.7.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mmucklo/email-parse": "^2.0",
        "propaganistas/laravel-phone": "^3.0",
        "symfony/validator": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
